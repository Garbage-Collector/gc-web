{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "noImplicitAny": false,
    "baseUrl": ".",
    "strict": false,
    "ignoreDeprecations": "5.0",
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "./dist/pwa",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ],
  "typeRoots": [
    "src/types"
  ]
}